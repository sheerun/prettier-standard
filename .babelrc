{
  "plugins": [
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    ["fast-async", {
      "runtimePatten": null,
      "compiler": {
        "promises": true,
        "es7": true,
        "noRuntime": true,
        "wrapAwait": true,
        "generators": false
      }
    }]
  ],
  "presets": [
    ["env", {
      "targets": {
        "node": 4.8
      }
    }]
  ]
}
