    "babel-eslint": ">=7.2.3",
    "babel-runtime": "^6.23.0",
    "chalk": "^2.3.0",
    "eslint": "^5.3.0",
    "find-up": "^3.0.0",
    "get-stdin": "^6.0.0",
    "glob": "^7.1.2",
    "ignore": "^5.0.1",
    "indent-string": "^3.1.0",
    "lodash.memoize": "^4.1.2",
    "loglevel-colored-level-prefix": "^1.0.0",
